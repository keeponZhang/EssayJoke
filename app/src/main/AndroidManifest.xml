<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	package="com.zhang.app">

	<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
	<uses-permission android:name="android.permission.INTERNET" />
	<uses-permission android:name="android.permission.USE_BIOMETRIC" />
	<uses-permission android:name="android.permission.USE_FINGERPRINT" />
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
	<uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
	<uses-sdk
		tools:overrideLibrary="android.support.design,android.support.v7.appcompat,android.support.v7.recyclerview,android.support.transition,android.support.fragment,android.support.core.ui
		,android.support.core.utils,android.support.v4,android.support.compat"/>
	<application
		android:name=".jump.MainApp"
		android:allowBackup="true"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name"
		android:supportsRtl="true"
		android:theme="@style/AppTheme">
		<activity android:name=".draw.DrawActivity"></activity>
		<activity android:name=".zhiwen.LoginSuccessActivity" />
		<activity android:name=".loader.LoaderActivity" />
		<activity android:name=".indicator.IndicatorActivity" />
		<activity android:name=".banner.BannerActivity" />
		<activity android:name="com.zhang.recyclerview.itemdecoration.ItemDecorationActivity" />
		<activity android:name="com.zhang.webview.WebViewActivity" />
		<activity android:name=".jump.JumpActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />

				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".indicator.ViewpagerActivity" />
		<activity android:name=".transitiondemo.FragmentCustomAnimationsActivity" />
		<activity android:name=".transitiondemo.TransitionActivity" />
		<activity android:name=".transitiondemo.CTTargetSupportActivity" />
		<activity android:name=".transitiondemo.CTTargetActivity" />
		<activity android:name=".zhiwen.LoginActivity" />
		<activity android:name=".draw.CustomSpanActivity" />
		<activity android:name=".aop.AopActivity" />
		<activity android:name=".lifecycle.LiveAppCompactActivity" />
		<activity android:name=".lifecycle.livedata.LiveDataActivity" />
		<activity android:name=".pingmu.DeviceActivity"/><!--android:theme="@style/AppTheme.FullScreen"-->

	</application>

</manifest>