<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zhang.support.sample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:name=".jump.MainApp"
        android:theme="@style/AppTheme">
	    <activity android:name=".SwipeRefreshActivity">

        </activity>
	    <activity android:name="com.android.coordinatorLayout.CoordinatorLayoutActivity"></activity>
	    <activity android:name="com.android.coordinatorLayout.CoordinatorLayoutToolBarActivity"></activity>
	    <activity
		    android:name="com.android.coordinatorLayout.CoordinatorLayoutAppBarLayoutNoBehaviorActivity"
		    android:theme="@style/Theme.AppCompat.NoActionBar"></activity>
	    <activity
		    android:name="com.android.coordinatorLayout.CoordinatorLayoutAppBarLayoutExituntilCollapsedActivity"
		    android:theme="@style/Theme.AppCompat.NoActionBar">
	    </activity>
	    <activity
		    android:name="com.android.coordinatorLayout.CoordinatorLayoutAppBarLayoutEnterAlwaysCollapsedActivity"
		    android:theme="@style/Theme.AppCompat.NoActionBar">
	    </activity>
	    <activity
		    android:name="com.android.coordinatorLayout.CoordinatorLayoutCollapsingToolbarLayoutActivity"
		    android:theme="@style/Theme.AppCompat.NoActionBar">
	    </activity>
		<activity
			android:name="com.android.coordinatorLayout.CoordinatorLayoutAppBarLayoutExituntilCollapsedRecyclerviewActivity"
			android:theme="@style/Theme.AppCompat.NoActionBar">
		</activity>
	    <activity android:name="com.android.coordinatorLayout.darren.DarrenBehaviorActivity"></activity>
	    <activity android:name=".jump.JumpActivity">
		    <intent-filter>
			    <action android:name="android.intent.action.MAIN" />

			    <category android:name="android.intent.category.LAUNCHER" />
		    </intent-filter>
	    </activity>
		<activity android:name="com.android.coordinatorLayout.DetailActivity"></activity>
		<activity android:name=".fragment.TestFragmentActivity"></activity>
		<activity android:name=".fragment.FragmentPagerAdapterBugActivity"></activity>
		<activity android:name=".fragment.FixPagerAdapterBugActivity"></activity>
	</application>

</manifest>