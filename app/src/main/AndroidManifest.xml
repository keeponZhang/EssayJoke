<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zhang.app">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".jump.MainApp">
        <activity android:name=".indicator.IndicatorActivity"></activity>
        <activity android:name=".banner.BannerActivity"></activity>
        <activity android:name="com.zhang.recyclerview.itemdecoration.ItemDecorationActivity">
        </activity>
        <activity android:name="com.zhang.webview.WebViewActivity">

        </activity>
        <activity android:name="com.zhang.app.jump.JumpActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>